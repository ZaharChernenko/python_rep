   "source": [
    "def static(digit: int):\n",
    "    def nested():\n",
    "        nonlocal digit\n",
    "        print(f\"Now i keep {digit}\")\n",
    "        digit += 1\n",
    "    return nested\n",
    "\n",
    "\n",
    "static1 = static(1)\n",
    "static1()\n",
    "static1()\n",
    "\n",
    "static2 = static(2)\n",
    "static2()\n",
    "static2()"
   ]
  }
